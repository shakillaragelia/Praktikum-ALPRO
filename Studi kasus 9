#include <iostream>
using namespace std;
class Andi {
public:
  void input();
  void proses();
  void output();

private:
  int data[24];
  int bil[4][6];
  int x, y, temp;
};
void Andi::input() {
  cout << "=====================================================" << endl;
  cout << "\t\tNOMOR BUKU YANG AKAN DIMASUKAN :" << endl;
  cout << "=====================================================" << endl;
  for (int i = 0; i < 24; i++) {
    cout << "Masukkan nomor buku ke " << i + 1 << " = ";
    cin >> data[i];
  }
  cout << endl;
}
void Andi::proses() {
  for (x = 0; x < 23; x++) {
    for (y = x + 1; y < 24; y++) {
      if (data[x] > data[y]) {
        temp = data[x];
        data[x] = data[y];
        data[y] = temp;
      }
    }
  }
