#include <iostream>
#include<stdio.h>
#include<iomanip>
#include <cstdlib>
#include<string>
using namespace std;

class matriks{
	friend ostream& operator<<(ostream&, matriks&);
    friend istream& operator>>(istream&, matriks&);
	public :
	  void input();
	  void proses();
	  void hasil();
	private :
	 string A[10][10];
	  int B[10][10];
	  int C[10];
	  int total;

};
void matriks::input(){
 int k,j;
  for(k=0;k<3;k++)
    for(j=0;j<3;j++){
      if(j==0){
        cout<<" Masukkan jurusan ke-"<<k<<" : ";
        cin>>(A[k][j]);
      }else if (j==1){
        cout<<" Masukkan Angkatan ke-"<<k<<" : ";
        cin>>A[k][j];
      }else if(j==2){
        cout<<" Masukkan Nilai ke-"<<k<<" : ";
        cin>>A[k][j];
      }
      }
  cout<<endl;
}

void matriks::proses(){ 
  int k,j; //BAGIAN CETAK
  for(k=0;k<3;k++){  //perulangan bersarang
    for(j=0;j<3;j++)
    cout<<setw(6)<<A[k][j]<< " ";
    cout<<endl;
    
  int k,j; //BAGIAN KONVERSI
  for(k=0;k<3;k++){
    for (j=1;j<3;j++)
      B[k][j]=atoi(A[k][j].c_str()); // ini fungsi dari library <cstdlib>
  }
  
}
}
